{
  "name": "three-5v5",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "type": "module",
  "scripts": {
    "predev": "node scripts/context/load.mjs",
    "dev": "vite",
    "postdev": "node scripts/context/save.mjs --append-log",
    "build": "tsc -b && vite build",
    "build:pages": "vite build",
    "postbuild": "node scripts/context/save.mjs --append-log",
    "prepreview": "node scripts/context/load.mjs",
    "preview": "vite preview",
    "postpreview": "node scripts/context/save.mjs --append-log",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "format": "prettier --write .",
    "bootstrap": "node scripts/bootstrap.mjs",
    "bootstrap:no-dev": "node scripts/bootstrap.mjs --no-dev",
    "bootstrap:fast": "node scripts/bootstrap.mjs --skip-test --with-dev",
    "ctx:save": "node scripts/context/save.mjs",
    "ctx:save:log": "node scripts/context/save.mjs --append-log",
    "ctx:load": "node scripts/context/load.mjs"
  },
  "dependencies": {
    "three": "^0.160.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/three": "^0.160.0",
    "@vitest/coverage-v8": "^1.4.0",
    "eslint": "^8.57.0",
    "jsdom": "^24.0.0",
    "prettier": "^3.3.0",
    "typescript": "^5.4.0",
    "vite": "^5.0.0",
    "vitest": "^1.4.0"
  }
}
